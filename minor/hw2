    "dict = {}\n",
    "\n",
    "while len(dict) < 10:\n",
    "    print(\"enter the country\")\n",
    "    country = input()\n",
    "    if dict.get(country) is None:\n",
    "        print(\"enter the capital of\", country)\n",
    "        capital = input()\n",
    "        dict.update({country: capital})\n",
    "    else:\n",
    "        print(\"the capital of\", country, \"is\", dict.get(country))\n",
    "\n",
    "print(dict)"
